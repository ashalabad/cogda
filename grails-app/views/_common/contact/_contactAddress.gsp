<div class="field address data show">
    <label class="viewAddressLbl">Mailing Address</label>
    <a class="btn btn-mini editAddress template" href="#" onclick="editAddress();">
        <i class="icon-edit"></i> Edit</a>
    <div class="addressOne">${address.address.addressOne}</div>
    <div class="addressTwo">${address.address.addressTwo}</div>
    <div class="addressThree">${address.address.addressThree}</div>
    <span class="city">${address.address.city}</span>, <span class="state">${address.address.state}</span> <span class="zipcode">${address.address.zipcode}</span>
</div>